algoritmo "Contador"

var
    contador, valor: inteiro

inicio
    contador <- 0
    escreva("Quer contar atÃ© quanto? ")
    leia(valor)
    Enquanto (contador <= valor) faca
    escreval("numero ", contador)
    contador <- (contador + 1)
    FimEnquanto
    escreval("terminei de escrever.")

fimalgoritmo