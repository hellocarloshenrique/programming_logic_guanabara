algoritmo "Nome do Programa"

var
    anoAtual, anoNasc, niver: inteiro

inicio
    escreva("Em que ano nos estamos? ")
    leia(anoAtual)
    escreva("Em que ano eu nasci? ")
    leia(anoNasc)
    niver <- anoAtual - AnoNasc
    escreva("Sabendo que você nasceu em ",anoNasc, " e estamos no ano de ", anoAtual,", sua idade é: ", niver)
    
fimalgoritmo