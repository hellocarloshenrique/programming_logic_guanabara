algoritmo "Nome do Programa"

var
    reais, cotDollar: real
     dollares: inteiro

inicio
    escreva("Você irá fazer uma viagem para os EUA, me diga quanto você tem em real? ")
    leia(reais)
    cotDollar <- 5.58
    dollares <- reais / cotDollar
    escreva("Com a cotação do dollar hoje em ", cotDollar, ", seu dinheiro em dolár será de ", dollares)

fimalgoritmo